_buildings = y_pred[:, 0].round()  # Binary classification\ny_pred_cropland = y_pred[:, 1].round()   # Binary classification\ny_pred_wcover = np.argmax(y_pred[:, 2:], axis=1)  # Convert one-hot encoding to class index\n\n# Evaluation\nprint(\"Buildings Accuracy:\", accuracy_score(y_test['building'], y_pred_buildings))\nprint(\"Cropland Accuracy:\", accuracy_score(y_test['cropland'], y_pred_cropland))\nprint(\"Woody Vegetation Accuracy:\", accuracy_score(np.argmax(y_test.iloc[:, 2:].values, axis=1), y_pred_wcover))\n\nprint(\"F1 Score (Buildings):\", f1_score(y_test['building'], y_pred_buildings))\nprint(\"F1 Score (Cropland):\", f1_score(y_test['cropland'], y_pred_cropland))\nprint(\"F1 Score (Woody Vegetation):\", f1_score(np.argmax(y_test.iloc[:, 2:].values, axis=1), y_pred_wcover, average='weighted'))","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-03-04T09:32:20.754678Z","iopub.execute_input":"2025-03-04T09:32:20.754990Z","iopub.status.idle":"2025-03-04T09:32:20.831560Z","shell.execute_reply.started":"2025-03-04T09:32:20.754967Z","shell.execute_reply":"2025-03-04T09:32:20.830761Z"}},"outputs":[{"name":"stdout","text":"Buildings Accuracy: 0.9433544303797469\nCropland Accuracy: 0.7727848101265823\nWoody Vegetation Accuracy: 0.5468354430379747\nF1 Score (Buildings): 0.4871060171919771\nF1 Score (Cropland): 0.6370070778564206\nF1 Score (Woody Vegetation): 0.5133171948763414\n","output_type":"stream"}],"execution_count":50},{"cell_type":"code","source":"# Get predicted probabilities (for both binary and multi-class outputs)\ny_pred_probs = np.column_stack([est.predict_proba(X_test) for est in model.estimators_])\n\n# For binary targets (building and cropland), the probabilities are two values:\n# Index 0: Probability for class 0 (e.g., 0)\n# Index 1: Probability for class 1 (e.g., 1)\ny_pred_probs_buildings = y_pred_probs[:, :2]  # Probabilities for buildings (class 0 and 1)\ny_pred_probs_cropland = y_pred_probs[:, 2:4]  # Probabilities for cropland (class 0 and 1)\n\n# For multi-class target (wcover), probabilities will be for each class:\n# wcover has 3 classes: wcover_<30%, wcover_>30%, wcover_>60%\ny_pred_probs_wcover = y_pred_probs[:, 4:].reshape(-1, 3)  # Probabilities for wcover (3 classes)\n\n# Evaluate performance based on predicted class labels (for accuracy/f1 score)\ny_pred_buildings = np.argmax(y_pred_probs_buildings, axis=1)\ny_pred_cropland = np.argmax(y_pred_probs_cropland, axis=1)\ny_pred_wcover = np.argmax(y_pred_probs_wcover, axis=1)\n\n# Calculate accuracy and F1 score\nprint(\"Buildings Accuracy:\", accuracy_score(y_test['building'], y_pred_buildings))\nprint(\"Cropland Accuracy:\", accuracy_score(y_test['cropland'], y_pred_cropland))\nprint(\"Woody Vegetation Accuracy:\", accuracy_score(np.argmax(y_test.iloc[:, 2:].values, axis=1), y_pred_wcover))\n\nprint(\"F1 Score (Buildings):\", f1_score(y_test['building'], y_pred_buildings))\nprint(\"F1 Score (Cropland):\", f1_score(y_test['cropland'], y_pred_cropland))\nprint(\"F1 Score (Woody Vegetation):\", f1_score(np.argmax(y_test.iloc[:, 2:].values, axis=1), y_pred_wcover, average='weighted'))\n\n# Display the predicted probabilities for the first few samples (optional)\nprint(\"\\nPredicted probabilities (Buildings, Cropland, Woody Vegetation):\")\nfor i in range(5):  # Show probabilities for the first 5 samples\n    print(f\"Sample {i+1}:\")\n    print(f\"  Building: {y_pred_probs_buildings[i]}\")\n    print(f\"  Cropland: {y_pred_probs_cropland[i]}\")\n    print(f\"  Woody Vegetation: {y_pred_probs_wcover[i]}\")\n    print()\n","metadata":{"trusted":true},"outputs":[],"execution_count":null}]}